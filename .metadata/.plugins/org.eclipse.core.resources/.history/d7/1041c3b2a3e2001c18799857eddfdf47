package com.example.Eccomerce.Exception;

public class IdNaoFornecidoException extends Exception{
	
	private String Message;

	public IdNaoFornecidoException(String message) {
		super();
		Message = message;
	}

	public String getMessage() {
		return Message;
	}

	public void setMessage(String message) {
		Message = message;
	}
	
	

}
