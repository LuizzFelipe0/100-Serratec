package estudo;

public class Produto {

	
	private int cod;
	private String descricao;
	private double valor;
	
	public Produto(String descricao) {
		this.descricao = descricao;
	}

	public String getDescricao() {
		return descricao;
	}

	public void setDescricao(String descricao) {
		this.descricao = descricao;
	}
	
}
