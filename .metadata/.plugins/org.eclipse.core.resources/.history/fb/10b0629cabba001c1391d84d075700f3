package exercicios;

public class Conta {
	
	String titular;
	 double numero;
	 double saldo;
	 boolean saque;
	 
	 public void deposito(double valor) {
		 this.saldo += valor;
		 System.out.println("O saldo atual Ã©: "+ this.saldo);
		 
	 }
	 public boolean saque(double valor) {
		 if(this.saldo < valor)
			 return false;
		 else {
			 this.saldo -= valor;
			 return true;
		 }
	 }
}
